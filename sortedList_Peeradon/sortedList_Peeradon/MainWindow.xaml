<Window x:Class="sortedList_Peeradon.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sortedList_Peeradon"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="addButton" Content="ADD" HorizontalAlignment="Left" Height="131" Margin="425,98,0,0" VerticalAlignment="Top" Width="250" Click="ADD"/>
        <TextBox x:Name="keyBox" HorizontalAlignment="Left" Height="22" Margin="65,98,0,0" TextWrapping="Wrap" Text="KEY" VerticalAlignment="Top" Width="234"/>
        <TextBox x:Name="valueBox" HorizontalAlignment="Left" Height="22" Margin="65,154,0,0" TextWrapping="Wrap" Text="VALUE" VerticalAlignment="Top" Width="234"/>

    </Grid>
</Window>
